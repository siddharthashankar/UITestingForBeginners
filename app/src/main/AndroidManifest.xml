    package="com.sid.uitest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recyclerviewui.RecyclerViewActivity"></activity>
        <activity android:name=".mockk.factory.MockMovieMainActivity" />
        <activity android:name=".dialog.DialogActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SecondaryActivity" />
        <activity android:name=".movieapp.ui.MainActivityMovie" />
        <activity android:name=".espressoidlingresource.ui.movie.EspressoIdlingResourceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>